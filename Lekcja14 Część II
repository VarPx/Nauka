public class Zwierze {
    //zemienna int liczebnosc
    static  int liczebnosc = 0;
    //kontruktor Zwierze
    Zwierze(){
        liczebnosc ++;
    }

    //zmienne String imie i glos
    String imie;
    String glos = "Grrr";

    //tworzenie metody dajGlos
    public void dajGlos(int x){
        for(int i = 0; i < x; i++){
            System.out.print(glos);
            System.out.print(" ");
            }
            System.out.println();
    }
    //tworzenie metody przedstaw sie
    public void przedstawSie(){
        System.out.println("Nazywam sie: " + imie);
    }
}
