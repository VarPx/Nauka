public class Lekcja17 {
    public static void main(String[] args){
        //utworzenie obiektu, przypsanie imienia i oceny
        Uczen u1 = new Uczen();
        u1.setNazwisko("Burchardt");
        System.out.println(u1.getNazwisko());
        u1.setNazwisko("k");
        System.out.println(u1.getNazwisko());

        System.out.println(u1.getOcena());
        u1.setOcena(4);
        System.out.println(u1.getOcena());
        u1.setOcena(9);
    }
}
